{
  "compilerOptions": {
    "outDir": "./dist/",
    "noImplicitAny": true,
    "module": "es6",
    "target": "es5",
    "jsx": "react-jsx",
    "allowJs": true,
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true, // import default from '파일'에 대한 허용
    "esModuleInterop": true // CommonJS 모듈 가져오기를 쉽게 지원하기 위한 옵션
  }
}
